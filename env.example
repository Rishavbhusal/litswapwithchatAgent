# Next.js Environment Variables
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Base Network Configuration (as used in terminal output)
NEXT_PUBLIC_RPC_URL=https://mainnet.base.org
NEXT_PUBLIC_CHAIN_ID=8453

# Trading Configuration  
NEXT_PUBLIC_UNISWAP_ROUTER_ADDRESS=0x68b3465833fb72A70ecDF485E0e4C7bD8665Fc45
NEXT_PUBLIC_DEFAULT_SLIPPAGE=0.5

# API Keys (Keep these secret - don't add NEXT_PUBLIC_ prefix)
PRIVATE_KEY=your_private_key_here
COINGECKO_API_KEY=your_coingecko_api_key
INFURA_PROJECT_ID=your_infura_project_id

# Development
NODE_ENV=development


# Vincent Platform Configuration
NEXT_PUBLIC_VINCENT_APP_ID=426578300

# Vincent Authentication
# The redirect URI for Vincent Connect (must match your app configuration)
NEXT_PUBLIC_VINCENT_REDIRECT_URI=http://localhost:3000/login

# Vincent App Delegatee Configuration
# Private key of the delegatee wallet that will sign Vincent Ability executions
VINCENT_APP_DELEGATEE_PRIVATE_KEY=your_delegatee_private_key_here